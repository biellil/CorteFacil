{
  "name": "CorteFacil",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "pnpm run start:web && pnpm run start:backend",
    "start:web": "cd web && pnpm preview",
    "start:backend": "cd API && node app.js",
    "dev": "concurrently \"pnpm run dev:web\" \"pnpm run dev:backend\"",
    "dev:web": "cd web && pnpm run dev",
    "dev:backend": "cd API && nodemon app.js",
    "build": "npm run build:web && npm run build:backend",
    "build:web": "cd web && pnpm run build",
    "build:backend": "cd API && pnpm run build"
  },
  "devDependencies": {
    "@rocketseat/eslint-config": "^2.2.2",
    "concurrently": "^8.2.2",
    "eslint": "^9.4.0"
  }
}