{
    "version": 2,
    "routes": [
      {
        "src": "^/login(?:/.*)?$",
        "dest": "/login/index.html",
        "continue": true
      },
      {
        "src": "/(.*)",
        "headers": {
          "cache-control": "s-maxage=1, stale-while-revalidate"
        },
        "dest": "/login/index.html"
      }
    ],
    "rewrites": [
      { "source": "/login/(.*)", "destination": "/login/index.html" }
    ]
  }
  