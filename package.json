{
  "name": "CorteFacil",
  "version": "1.0.0",
  "private": true,
"scripts": {
  "start": "pnpm run start:web && pnpm run start:api",
  "start:web": "cd web && pnpm preview",
  "start:api": "cd api && npm start",
  "dev": "concurrently \"pnpm run dev:web\" \"pnpm run dev:api\"",
  "dev:web": "cd web && pnpm run dev",
  "dev:api": "cd api && npm run start:dev",
  "build": "npm run build:web && npm run build:api",
  "build:web": "cd web && pnpm run build",
  "build:api": "cd api && npm run build"
},
  "devDependencies": {
    "@rocketseat/eslint-config": "^2.2.2",
    "concurrently": "^8.2.2",
    "eslint": "^9.4.0"
  }
}